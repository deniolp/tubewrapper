{
  "name": "tubewrapper",
  "version": "1.0.0",
  "description": "Youtube wrapper. Learning Web Components and Google Youtube API",
  "main": "index.js",
  "scripts": {
    "test": "stylelint 'style.css' && eslint *.js",
    "start": "gulp serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deniolp/tubewrapper.git"
  },
  "keywords": [
    "web-components",
    "api",
    "youtube",
    "grids"
  ],
  "author": "Denis Popov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deniolp/tubewrapper/issues"
  },
  "homepage": "https://github.com/deniolp/tubewrapper#readme",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "eslint": "^5.16.0",
    "eslint-config-htmlacademy": "^0.5.2",
    "gulp": "^4.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-htmlacademy": "^0.1.3"
  },
  "stylelint": {
    "extends": "stylelint-config-htmlacademy",
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignore": "custom-elements"
        }
      ],
      "selector-max-id": 1
    }
  },
  "dependencies": {
    "axios": "^0.21.2",
    "eslint-utils": "^1.4.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6"
  }
}
